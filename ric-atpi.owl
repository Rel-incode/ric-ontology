        <ric:subjectTo rdf:resource="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#territorialapplication"/>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#rightreimbursementreroutingcompelled -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#rightreimbursementreroutingcompelled">
        <rdf:type rdf:resource="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric.owl#Right"/>
        <rdfs:label xml:lang="en">Right to reimbursement or rerouting</rdfs:label>
        <ric:hasEnforcementProcedure rdf:resource="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#complainthandling"/>
