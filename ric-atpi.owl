        <ric:subjectTo rdf:resource="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#reimbursementconstraint"/>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#rightreimbursementreroutingdelaydeparture -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric-atpi.owl#rightreimbursementreroutingdelaydeparture">
        <rdf:type rdf:resource="https://raw.githubusercontent.com/Rel-incode/ric-ontology/master/ric.owl#Right"/>
